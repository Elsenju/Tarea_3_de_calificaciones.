#include <stdio.h>

main()
{
    char  final1, final2, final3;
    float prom, nota1, nota2, nota3;
    printf ("ingresa tus notas dadas del fin del curso.\n");
    printf ("Teniendo en cuenta que cada letra tiene un valor.\n");
    printf ("A=10.\n");
    printf ("B=9.\n");
    printf ("C=8.\n");
    printf ("D=7.\n");
    printf ("F=6.\n");
    printf ("G=5.\n");
    printf("Digita tus 3 calificaciones por favor:\n");

    scanf("%c %c %c", &final1, &final2, &final3);

    if (final1=='A')
    {
        nota1=10;

    }
    else if (final1=='B')
    {
        nota1=9;
    }
    else if (final1=='C')
    {
        nota1=8;
    }
    else if (final1=='D')
    {
        nota1=7;
    }
    else if (final1=='F')
    {
        nota1=6;
    }
    else if (final1=='G')
    {
        nota1=5;
    }

 else
        {
        printf ("Calificacion  reprobatoria.\n");
    }

    if (final2=='A')
    {
        nota2=10;

    }
    else if (final2=='B')
    {
        nota2=9;
    }
    else if (final2=='C')
    {
        nota2=8;
    }
    else if (final2=='D')
    {
        nota2=7;
    }
    else if (final2=='F')
    {
        nota2=6;
    }
    else if (final2=='G')
    {
        nota2=5;
    }

 else
        {
        printf ("Calificacion  reprobatoria\n");
    }

       if (final3=='A')
    {
        nota3=10;

    }
    else if (final3=='B')
    {
        nota3=9;
    }
    else if (final3=='C')
    {
        nota3=8;
    }
    else if (final3=='D')
    {
        nota3=7;
    }
    else if (final3=='F')
    {
        nota3=6;
    }
    else if (final3=='G')
    {
        nota3=5;
    }
 else
    {
        printf ("Calificacion reprobatoria %f \n", prom);
    }
    prom=(nota1+nota2+nota3)/3;
    printf ("Tu promedio es de:%f\n", prom);
if (prom<=6)
    {
        printf ("Calificacion reprobatoria\n");
    }

    return 0;
}
